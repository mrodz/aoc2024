mul        = @{ "mul" }
operation  = ${ mul }

number     = @{ ASCII_DIGIT+ }

lpar       = @{ "(" }
rpar       = @{ ")" }
comma      = @{ "," }

arguments  = ${ lpar ~ ((number ~ (comma ~ number)+) | number)? ~ rpar }
invocation = ${ operation ~ arguments }

file       = ${ SOI ~ (invocation | ANY)* ~ EOI }
